{{#if versionRecord.modulesSorted.length}}
  <h2>Modules</h2>

  <ul>
    {{#each versionRecord.modulesSorted as |module|}}
      <li>
        {{link-to module.name 'version.module' module.id}}
      </li>
    {{/each}}
  </ul>
{{/if}}

{{#if versionRecord.namespacesSorted.length}}
  <h2>Namespaces</h2>

  <ul>
    {{#each versionRecord.namespacesSorted as |namespace|}}
      <li>
        {{link-to namespace.name 'version.namespace' namespace.id}}
      </li>
    {{/each}}
  </ul>
{{/if}}

{{#if versionRecord.classesSorted.length}}
  <h2>Classes</h2>

  <ul>
    {{#each versionRecord.classesSorted as |klass|}}
      <li>
        {{link-to klass.name 'version.class' klass.id}}
      </li>
    {{/each}}
  </ul>
{{/if}}